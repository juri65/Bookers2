
<!--User 一覧 -->
<div class="col-md-8 offset-md-1">
    <h2>Users</h2>
    <table class="table">
        <thead>
            <tr>
                <th>image</th>
                <th>name</th>
                <th colspan="3"></th>
            </tr>
        </thead>
        <tbody>
            <% @users.each do |user| %>
            <%= image_tag @user.get_profile_image(100,100) %>
            <%= link_to user_path(user) do %>
            
        </tbody>
    </table>
</div>
